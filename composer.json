{
    "name": "sindria/homestead",
    "description": "A fork of laravel/homestead for every php development environment.",
    "require": {
        "php": "^5.6 || ^7.0",
        "symfony/console": "~3.3|~4.0",
        "symfony/process": "~3.3|~4.0",
        "symfony/yaml": "~3.3|~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0",
        "phly/changelog-generator": "^2.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sindria Inc.",
            "email": "sindriaproject@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sindria\\Homestead\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "bin": [
        "bin/homestead"
    ]
}
